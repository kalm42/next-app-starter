{
  "name": "next-app-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "db:up": "docker-compose -f docker-compose.yaml up -d",
    "db:down": "docker-compose -f docker-compose.yaml down --volumes --remove-orphans",
